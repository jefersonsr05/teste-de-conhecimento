  inherited qryListagem: TZQuery
    SQL.Strings = (
      
        'select v.nrnota, v.emissao, v.cliente,c.nome, v.tipo_venda, v.va' +
        'lor_venda, v.operacao_venda'
      'from venda v'
      'inner join cliente c on c.codigo = v.cliente')
